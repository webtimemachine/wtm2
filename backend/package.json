  "scripts": {
    "postinstall": "prisma generate",
    "build": "prisma generate && nest build && npm run sentry:sourcemaps && npm run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "migrate": "prisma migrate dev --skip-seed",
    "migrate:create": "prisma migrate dev --name",
    "migrate:create-only": "prisma migrate dev --create-only --name",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "sentry:sourcemaps": "./sentry-sourcemaps.sh"
  },
  "dependencies": {
    "@langchain/community": "^0.0.36",
