    "@nestjs/core": "^10.3.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.2",
    "@prisma/client": "^5.8.1",
    "@sentry/cli": "^2.32.1",
