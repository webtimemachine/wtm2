    "@sentry/profiling-node": "^8.12.0",
    "@sentry/tracing": "^7.114.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.0",
    "graphql": "^16.8.1",
    "handlebars": "^4.7.8",
    "jest-mock-extended": "^3.0.5",
    "langchain": "^0.1.26",
    "nodemailer": "^6.9.13",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
